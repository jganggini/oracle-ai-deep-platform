# Configuración para MinerU Docker
MINERU_PORT=8001

# GPU
GPU_ENABLED=true
GPU_DEVICE=cuda
GPU_BACKEND=pipeline

# Optimizaciones de hilos para evitar contención
OMP_NUM_THREADS=1
MKL_NUM_THREADS=1
OPENBLAS_NUM_THREADS=1
NUMEXPR_NUM_THREADS=1

# Configuración de caché y modelos
CACHE_DIR=/app/cache
MODEL_PATH=/app/models